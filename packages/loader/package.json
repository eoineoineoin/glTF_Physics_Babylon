{
  "name": "babylon-gltf-rigid-body-loader",
  "version": "0.5.4",
  "description": "Plugins for loading glTF files with rigid body data in Babylon.JS",
  "repository": "github:eoineoineoin/glTF_Physics_Babylon",
  "main": "dist/babylon-gltf-rigid-body-loader.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:umd && npm run build:esm",
    "clean": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "build:esm": "tsc -b",
    "build:umd": "rollup -c"
  },
  "author": "Eoin Mcloughlin",
  "license": "MIT",
  "peerDependencies": {
    "@babylonjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@babylonjs/loaders": "^8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "rollup": "^3.29.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/*",
    "LICENSE.txt",
    "README.md"
  ]
}
